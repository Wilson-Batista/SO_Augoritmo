<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.so.telas.MainMenu">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Text layoutX="179.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Bem vindo ao simulador de escalonamento" />
                <Button fx:id="rr" layoutX="243.0" layoutY="92.0" mnemonicParsing="false" onAction="#rrConfig" text="Simular Round Robin" />
                <Button fx:id="ssjf" layoutX="274.0" layoutY="145.0" mnemonicParsing="false" onAction="#sjfConfig" text="Simular SJF" />                                
                <Button fx:id="results" layoutX="275.0" layoutY="317.0" mnemonicParsing="false" onAction="#showResults" text="Resultados" />
            </children>
        </AnchorPane>
    </children>
</VBox>
